<h1>New users.
</h1>

<%= form_for @user do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :hospital %>
    <%= f.text_field :hospital %>

    <%= f.label :dob %>
    <%= f.text_field :dob %>

    <%= f.submit "Submit" %>
  </div>

<% end %>